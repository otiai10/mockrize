{
  "name": "mockrize",
  "version": "0.1.1",
  "description": "Create REST API mock server from based on your repository",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "directories": {
    "example": "example"
  },
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.2",
    "@types/supertest": "^2.0.8",
    "jest": "^25.4.0",
    "supertest": "^4.0.2",
    "ts-jest": "^25.4.0",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "build": "tsc -p ./tsconfig.json",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/otiai10/mockrize.git"
  },
  "author": "otiai10",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/otiai10/mockrize/issues"
  },
  "homepage": "https://github.com/otiai10/mockrize#readme",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/tests/**/*.test.ts"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/example/"
    ]
  }
}
